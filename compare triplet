int* compareTriplets(int a_count, int* a, int b_count, int* b, int* result_count) {
    int *resultcount=(int*)calloc(2,sizeof(int));
    *result_count=2;
    if(a_count==0&&b_count==0){
        return NULL;
    }
    else{
        for(int i=0;i<3;i++){
            if(*(a+i)>*(b+i)){
                resultcount[0]+=1;
            }
            else if(*(a+i)<*(b+i)){
                resultcount[1]+=1;
            }
        }
        return resultcount;
    }
}
